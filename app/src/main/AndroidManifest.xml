<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/tm_logo"
        android:label="@string/app_name"
        android:name=".presentation.di.TaskMasterApplication"
        android:roundIcon="@drawable/tm_logo"
        android:supportsRtl="true"
        android:theme="@style/Theme.TaskMasterCompose"
        tools:targetApi="31">
        <activity
            android:name=".presentation.ui.activities.MainActivity"
            android:exported="true"
            android:theme="@style/Theme.TaskMasterCompose">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>